<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

	<display-name>TagCloud</display-name>
	<servlet>
		<servlet-name>crawl</servlet-name>
		<display-name>crawl</display-name>
		<description></description>
		<jsp-file>/crawl.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>crawl</servlet-name>
		<url-pattern>/crawl</url-pattern>
	</servlet-mapping>
	<session-config>
		<session-timeout>
			30
		</session-timeout>
	</session-config>
	
	<welcome-file-list>
		<welcome-file>
			index.jsp
		</welcome-file>
	</welcome-file-list>
	



</web-app>
